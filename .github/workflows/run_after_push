name: Build after push

on:
  workflow_dispatch:
  push:
    branches: ["master"]
    
    

jobs:
  build:
    runs-on: windows-latest
    permissions:
      contents: read

    steps:
      uses: actions/checkout@v4

    - name: Set up .NET Core
      uses: actions/setup-dotnet@v4
      with:
        dotnet-version: '9.x'

    - name: Build with dotnet
        run: dotnet build --configuration Release
